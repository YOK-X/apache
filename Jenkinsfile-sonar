pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('SonarQube analysis') {
        def scannerHome = tool 'SonarQube';
        withSonarQubeEnv('My SonarQube Server') { 
            sh "${scannerHome}/bin/sonar-scanner"
        }
      }
    }
  }
